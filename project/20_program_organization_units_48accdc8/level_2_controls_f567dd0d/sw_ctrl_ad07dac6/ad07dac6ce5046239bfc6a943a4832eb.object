ve;\n\tSW.Ready:=FALSE;\n\tSW.Active:=FALSE;\nEND_IF;\n\nIF NOT(Pump_Ready) THEN\n\tSW.Status:=Transfer_Status.Pump_Not_Ready;\n\tSW.Ready:=FALSE;\n\tSW.Active:=FALSE;\nEND_IF\n\n\nIF NOT(BCV.Ready) THEN\n\tSW.Status:=Transfer_Status.BCV_Not_Ready;\n\tSW.Ready:=FALSE;\n\tSW.Active:=FALSE;\nEND_IF;\n\nIF (BCV.Status.eStatus=eBCV_Status.Interlocked OR BCV.Status.eStatus=eBCV_Status.LOCKED_OUT ) THEN\n\tSW.Status:=Transfer_Status.LOCKED_OUT;\n\tSW.Ready:=FALSE;\n\tSW.Active:=FALSE;\nEND_IF;\n\n\nIF Sw.Status=Transfer_Status.IDLE AND Interlock THEN\n\tSW.Status:=Transfer_Status.LOCKED_OUT;\nEND_IF\n\nCASE SW.Status OF\n\t\n\tTransfer_Status.IDLE:\n\t\tSW.Ready:=TRUE;\n\t\tSW.BCV:=FALSE;\n\t\t\n\t\tSW.Remaining_Time_S:=0;\n\t\tSW.Current_Rate:=0;\n\t\tSW.Current_Volume:=0;\n\t\tSW.Remaining_Volume:=0;\n\t\tSW.Active:=FALSE;\n\t\t\n\t\tIF (\n\t\t\t(SW.Auto_Batch AND SW.Mode.eMode=eBatch_Transfer_Mode.Auto) OR\n\t\t\t( SW.Mode.eMode=eBatch_Transfer_Mode.Batch)\n\t\t\t)THEN\n\t\t\tSW.Status:=Transfer_Status.FIT_Reset;\n\t\tEND_IF;\n\t\t\n\t\t\n\tTransfer_Status.FIT_Reset:\n\t\tSW.Active:=TRUE;\n\t\tSW.BCV:=FALSE;\n\t\tSW.Remaining_Time_S:=0;\n\t\tFIT.PLC_Reset:=(FIT.Total>1);\n\t\tIF FIT.Total<1 THEN\n\t\t\tSW.Status:=Transfer_Status.Open_Valve;\n\t\tEND_IF;\n\t\t\n\tTransfer_Status.Open_Valve:\n\tSW.BCV:=TRUE;\n\tIF SW.BCV AND BCV.Status.eStatus=eBCV_Status.A_Open THEN\n\t\t\tSW.Status:=Transfer_Status.Flow_Active;\t\n\t\tEND_IF;\n\t\t\n\t\n\t\t\n\tTransfer_Status.Flow_Active:\n\t\tSW.BCV:=TRUE;\n\t\tSW.Remaining_Time_S:=TO_UDINT(SW.Remaining_Volume/(FIT.Rate.EU/60));\n\t\tSW.Current_Rate:=FIT.Rate.EU;\n\t\tSW.Current_Volume:=FIT.Total;\n\t\tSW.Remaining_Volume:=(SW.Volume-FIT.Total);\n\t\t\n\t\t\n\t\tIF (\n\t\t\t(FIT.Rate.EU > (SW.Rate*0.25)) OR\n\t\t\t(FIT.Total> (SW.Volume*0.80))\n\t\t) THEN\n\t\t\tSW.Status:=Transfer_Status.Running;\n\t\tEND_IF;\n\tTransfer_Status.Running:\t\n\t\t\tSW.BCV:=TRUE;\n\t\t\tSW.Remaining_Time_S:=TO_UDINT(SW.Remaining_Volume/(FIT.Rate.EU/60));\n\t\t\tSW.Current_Rate:=FIT.Rate.EU;\n\t\t\tSW.Current_Volume:=FIT.Total;\t\n\t\t\tSW.Remaining_Volume:=(SW.Volume-FIT.Total);\n\t\t\tIF (FIT.Total> SW.Volume)\n\t\t\tTHEN\n\t\t\t\tSW.Status:=Transfer_Status.Complete;\n\t\t\tEND_IF;\n\tTransfer_Status.Complete:\n\t\tSW.BCV:=FALSE;\n\t\tSW.Remaining_Time_S:=0;\n\t\tSW.Current_Rate:=0;\n\t\tSW.Current_Volume:=0;\n\t\tSW.Remaining_Volume:=0;\n\t\tSW.Active:=FALSE;\n\t\t\n\tTransfer_Status.LOCKED_OUT:\n\t\tSW.Ready:=FALSE;\n\t\tSW.Active:=FALSE;\n\t\tSW.Remaining_Time_S:=0;\n\t\tSW.Current_Rate:=0;\n\t\tSW.Current_Volume:=0;\n\t\tSW.Remaining_Volume:=0;\n\t\t\n\t\t\n\tTransfer_Status.Destination_Not_Ready:\n\t\tSW.Active:=FALSE;\n\t\tIF SW.BCV AND BCV.Status.eStatus=eBCV_Status.A_Open THEN\n\t\t\tSW.Status:=Transfer_Status.Flow_Active;\t\t\n\t\tEND_IF;\n\t\tIF \t(\n\t\t\t\tNOT(SW.Mode.eMode=eBatch_Transfer_mode.Auto AND SW.Auto_Batch = TRUE) AND\n\t\t\t\tNOT(SW.Mode.eMode=eBatch_Transfer_mode.Batch) AND\n\t\t\t\tSW.BCV=TRUE\n\t\t\t)THEN\n\t\t\tSW.BCV:=FALSE;\n\t\tEND_IF;\n\tTransfer_Status.Flow_Meter_Active:\n\t\tIF FIT.Rate.EU<0.5 THEN\n\t\t\tSW.Status:=Transfer_Status.IDLE;\n\t\tEND_IF;\nEND_CASE;\n\n\n\nPID(\n\tIN:= (SW.Rate-FIT.Rate.EU), \n\tKP:= SW.Config.KP, \n\tTN:= SW.Config.TN, \n\tTV:= SW.Config.TV, \n\tLIM_L:= 0, \n\tLIM_H:= 100-(SW.Config.FF*SW.Rate), \n\tRST:= NOT(SW.Active) OR NOT(BCV.Status.eStatus=eBCV_Status.A_Open) , \n\tY=> , \n\tLIM=> );\n\t\nIF NOT(SW.Active) THEN\n\tSW.ACV:=0;\n\tSW.BCV:=FALSE;\nELSE\n\tSW.ACV:=PID.Y+(SW.Config.FF*SW.Rate);\n\nEND_IF;\n","LineInfoPersistence":"(string)ad07dac6-ce50-4623-9bfc-6a943a4832eb_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION_BLOCK SW_CTRL \nVAR_INPUT\n\tSW: REFERENCE TO cBatch_Transfer;\n\tFIT:REFERENCE TO cFIT;\n\tBCV:REFERENCE TO cBCV;\n\tPump_Ready:BOOL;\n\tInterlock:BOOL;\nEND_VAR\nVAR\n\tPID: BURKERT_OSCAT_BASIC.FT_PIDW;\nEND_VAR\n","LineInfoPersistence":"(string)ad07dac6-ce50-4623-9bfc-6a943a4832eb_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)242","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}