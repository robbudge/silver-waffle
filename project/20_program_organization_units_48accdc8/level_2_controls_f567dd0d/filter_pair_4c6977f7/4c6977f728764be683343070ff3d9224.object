2:\n\t\tTask_Ton.IN:=(Filter.Filter_1.Status.eStatus=eFilter_Status.In_Service AND Filter.Filter_2.Status.eStatus=eFilter_Status.In_Service AND NOT(Filter.DP.Status.eStatus=eSensor_Status.HiHi_Alarm));\n\t\t\n\t\tIF \tTask_Ton.Q THEN\n\t\t\tFilter.Status:=eFilter_Manager_Status.Filter_2_Only; \n\t\t\tFilter.Filter_1.High_DP:=TRUE;\n\t\tEND_IF;\n\t\n\teFilter_Manager_Status.Enable_Filter_2:\n\t\tTask_Ton.IN:=FALSE;\n\t\tIF Filter.Filter_2.Status.eStatus=eFilter_Status.In_Service THEN  Filter.Status:=eFilter_Manager_Status.Filter_2_Active; END_IF;\n\t\n\teFilter_Manager_Status.Filter_2_Active:\n\t\tTask_Ton.IN:=FALSE;\n\t\tIF Filter.DP.Status.eStatus=eSensor_Status.HiHi_Alarm THEN Filter.Status:=eFilter_Manager_Status.Filter_2_x_1; END_IF;\n\t\t\n\teFilter_Manager_Status.Filter_2_x_1:\n\t\tTask_Ton.IN:=(Filter.Filter_1.Status.eStatus=eFilter_Status.In_Service AND Filter.Filter_2.Status.eStatus=eFilter_Status.In_Service AND NOT(Filter.DP.Status.eStatus=eSensor_Status.HiHi_Alarm) );\n\t\tIF Task_Ton.Q THEN \n\t\t\tFilter.Status:=eFilter_Manager_Status.Filter_1_Only; \n\t\t\tFilter.filter_2.High_DP:=TRUE;\n\t\tEND_IF;\n\t\t\t\t\n\teFilter_Manager_Status.Filter_1_Only:\n\t\tTask_Ton.IN:=FALSE;\n\t\tIF Filter.Filter_2.Ready AND Filter.Mode.eMode=eFilter_manager_modes.Auto THEN\t\n\t\t\tfilter.Status:=eFilter_Manager_Status.Filter_1_Active; END_IF;\n\teFilter_Manager_Status.Filter_2_Only:\t\n\t\tIF Filter.Filter_1.Ready AND Filter.Mode.eMode=eFilter_manager_modes.Auto THEN\t\n\t\t\tfilter.Status:=eFilter_Manager_Status.Filter_2_Active; \n\t\tEND_IF;\n\t\n\teFilter_Manager_Status.No_Filter_Available:\n\t\tTask_Ton.IN:=FALSE;\n\t\tIF Filter.Filter_1.Ready AND Filter.Mode.eMode=eFilter_manager_modes.Auto THEN\t\n\t\t\tfilter.Status:=eFilter_Manager_Status.Filter_1_Only;\n\t\tEND_IF;\n\t\tIF Filter.Filter_2.Ready AND Filter.Mode.eMode=eFilter_manager_modes.Auto THEN\t\n\t\t\tfilter.Status:=eFilter_Manager_Status.Filter_2_Only;\n\t\tEND_IF;\n\t\t\n\teFilter_Manager_Status.Disabled:\n\t\tFilter.Filter_1.Status.eStatus:=eFilter_Status.Disabled;\n\t\tFilter.Filter_2.Status.eStatus:=eFilter_Status.Disabled;\n\t\t//eFilter_Manager_Status.No_Filter_Available:\n\t\nEND_CASE\n\nMode();\nFilter_Control();\n\n\n\nfilter.strStatus:=Burkert_OSCAT_BASIC.REPLACE_ALL(str:=TO_STRING(Filter.Status), src:='_' , rep:=' ' );\n","LineInfoPersistence":"(string)4c6977f7-2876-4be6-8334-3070ff3d9224_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION_BLOCK Filter_Pair\nVAR_INPUT\n\tFilter: Reference to cDuplex_Filter;\nEND_VAR\nVAR_OUTPUT\nEND_VAR\nVAR\n\tAlarm:Analog_Alarm_UDFB;\n\tR_TRIG_0: R_trig;\n\tIdle_mode: R_trig;\n\tFilter_1_Active: R_trig;\n\tFilter_1_Only: R_trig;\n\tFilter_2_Active: R_trig;\n\tFilter_2_Only: R_trig;\n\tFilter_1x2: R_trig;\n\tFilter_2x1: R_trig;\n\tEnable_1: R_trig;\n\tEnable_2: R_trig;\n\tNon_Available: R_trig;\n\tFilter_1_Manual: R_trig;\n\tFilter_2_Manual: R_trig;\n\tF_Trig_1x2: F_Trig;\n\tF_Trig_2x1: F_Trig;\n\tAuto_r_trig:R_Trig;\n\t\n\tTask_ton:Ton;\n\t\n\t\n\tGroup_Disable: R_trig;\n\tGroup_Bypass: R_trig;\nEND_VAR\n","LineInfoPersistence":"(string)4c6977f7-2876-4be6-8334-3070ff3d9224_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)137","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}