ll.8:=Flash.Q;\n\t\tIF Flash.Q THEN Stage:=5; END_IF;\n\t5: \tIF NOT(Flash.Q) THEN Stage:=6; END_IF;\n*)\n\t6:\tRaw_Data:=ORF.Value_RX;\n\t\tIF Raw_Data<16#FFFFFFFE\tTHEN \n\t\t\tData.tDword:=MEM.ReverseWORDsInDWORD(dwInput:=Raw_Data );\n\t\t\tStage:=7; \n\t\tEND_IF;\n\t\tOrf.Call.8:=TRUE;\n\t\tIF ORF.Result=16#FFFFFFFF THEN Active_count:=Active_count+1; END_IF;\n\t\tIF ORF.Result>1 AND ORF.Result<16#FFFFFFFF AND Flash.Q\tTHEN Stage:=9; END_IF;\n\t\t\n\t7:\t//First_data:=MEM.ReverseWORDsInDWORD(dwInput:=orf.Value_RX );\n\t\tORF.Value_TX:=0;\n\t\tOrf.Call:=16#0000;\n\t\tORF.Write_Value:=FALSE;\n\t\tRaw_Data:=16#FFFFFFFD;\n\t\tStage:=8;\n\t8:\t\n\t\tComplete:=TRUE;\n\t\tORF.Write_Value:=FALSE;\n\t\tOrf.Call:=16#0000;\t\t\n\t9: \n\t\tFault_Code:=MEM.ReverseWORDsInDWORD(dwInput:=orf.Result );\n\t\tData.tDword:=0;\n\t\tORF.Index_TX:=Build_Orf_Index(Index:=16#1000,SubIndex:=16#0,Node:=Node,Write:=FALSE);\n\t\tFault_Count:=Fault_Count+1;\n\t\tStage:=10;\n\t10:\n\t\tOrf.Call.8:=Flash.Q;\n\t\tIF orf.Result=0 THEN stage:=11;END_IF;\n\t11:\tOrf.Call:=0;\n\t\tORF.Index_TX:=Build_Orf_Index(Index:=Index,SubIndex:=SubIndex,Node:=Node,Write:=FALSE);\n\t\tComplete:=TRUE;\nEND_CASE","LineInfoPersistence":"(string)120ba057-b7eb-47e2-ae7c-d6dbe0d1f2f6_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION_BLOCK eDip_ReadSingleItem\nVAR_INPUT\n    Index     : WORD;\n    SubIndex  : BYTE;\n    Node      : BYTE;\n\tEnable:BOOL;\n\torf:REFERENCE TO eDip_MEx3_ORF;\nEND_VAR\nVAR_OUTPUT\n\tData:Bytes_Dword;\n\tComplete:BOOL;\nEND_VAR\nVAR\n\tStage:BYTE:=0;\n\tFlash:Burkert_OSCAT_BASIC.GEN_PULSE;\n\tActive_count:UINT:=0;\n\tFault_Count:UINT:=0;\n\tFault_Code:DWORD;\n\tRaw_Data:Dword;\n\t\nEND_VAR\n","LineInfoPersistence":"(string)120ba057-b7eb-47e2-ae7c-d6dbe0d1f2f6_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)110","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type"