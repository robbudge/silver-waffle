{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)9d01ef71-0294-48ec-b114-187911936bbe","ParentGuid":"(Guid)72f82f92-2c75-4f30-8db8-f04ec70d927d","Name":"(string)Status","Properties":{"@Type":"2c41fa04:IDictionary","@Value":{}},"TypeGuid":"(Guid)8ac092e5-3128-4e26-9e7e-11016c6684f2","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"8ac092e5","@Value":{"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)lclAlarm_Active(Reset1:=(VFD.MODE.eMode=VFD_modes.Reset OR NOT(Current_Alarm_EN) ));\n\nVFD.Ready:=(\n\t(CommState=ModbusTCPComState.CONNECTED OR CommState=ModbusTCPComState.OFF) AND \n\t(VFD.MODE.eMode=VFD_Modes.Auto OR VFD.MODE.eMode=VFD_Modes.Simulate) AND \n\tVFD.Data.Drive_STATUS.Ready AND\n\tNOT(lclAlarm_Active.Q1) );\n\t\nVFD.Status.eStatus:=eVFD_Status.READY;\n\n\n\nIF VFD.Data.Drive_STATUS.Run AND VFD.MODE.eMode=VFD_Modes.Manual AND VFD.Data.Drive_STATUS.At_Speed THEN\tVFD.Status.eStatus:=eVFD_Status.M_Runing; END_IF;\nIF VFD.Data.Drive_STATUS.Run AND VFD.MODE.eMode=VFD_Modes.Auto AND VFD.Data.Drive_STATUS.At_Speed THEN VFD.Status.eStatus:=eVFD_Status.A_Running; END_IF;\nIF VFD.Data.Drive_STATUS.Run AND VFD.MODE.eMode=VFD_Modes.Simulate AND VFD.Data.Drive_STATUS.At_Speed THEN VFD.Status.eStatus:=eVFD_Status.A_Running; END_IF;\nIF VFD.Data.Drive_STATUS.Run  AND NOT(VFD.Data.Drive_STATUS.At_Speed) THEN VFD.Status.eStatus:=eVFD_Status.Ramping; END_IF;\nIF VFD.MODE.eMode=VFD_Modes.Auto AND NOT(Permissive_NC) THEN VFD.Status.eStatus:=eVFD_Status.Interlocked; END_IF;\n\nIF VFD.Data.Drive_STATUS.Fault THEN \n\tVFD.Status.eStatus:=eVFD_Status.Fault; \n\tVFD_CMD_PCT:=0;\n\tVFD_Run:=FALSE;\nEND_IF;\n\nIF VFD.MODE.eMode=VFD_Modes.Disabled THEN VFD.Status.eStatus:=eVFD_Status.Disabled; END_IF;\nIF NOT(Permissive_NC) THEN \tVFD.Status.eStatus:=eVFD_Status.Interlocked; END_IF;\nIF NOT(Interlock_NC) THEN \n\t\tVFD.Status.eStatus:=eVFD_Status.LOCKED_OUT; \n\t\tVFD_CMD_PCT:=0;\n\t\tVFD_Run:=FALSE;\nEND_IF;\n\n\n\nIF VFD.Data.DC_Link<100 THEN\tVFD.Status.eStatus:=eVFD_Status.Low_Voltage; END_IF;\nIF VFD.Data.Fault_Code=66 THEN VFD.Status.eStatus:=eVFD_Status.Safe_Torque_Off; END_IF;\n\nIF NOT(CommState=ModbusTCPComState.CONNECTED OR CommState=ModbusTCPComState.OFF)\tAND NOT(VFD.MODE.eMode=VFD_modes.Simulate)\tTHEN \n\tVFD.Status.eStatus:=eVFD_Status.Offline;\n\tVFD_CMD_PCT:=0;\n\tVFD.Output_Pct:=0;\n\tVFD_Run:=FALSE;\nEND_IF;\n\n\nCASE CommState OF\n\tModbusTCPComState.OFF: VFD.strTCPstate:='OFF';\n\tModbusTCPComState.CONNECTING: VFD.strTCPstate:='CONNECTING';\n\tModbusTCPComState.CONNECTED: VFD.strTCPstate:='CONNECTED';\n\tModbusTCPComState.DISCONNECTING: VFD.strTCPstate:='DISCONNECTING';\n\tModbusTCPComState.SOCKET_ERROR: VFD.strTCPstate:='SOCKET_ERROR';\nEND_CASE\n\n\n\nVFD.Current_Alarm.Silence:=VFD.Output_Pct<25;\nAlarm(Config:=VFD.Current_Alarm,eu:=VFD.Data.Motor_Current,Debounce_Active:=FALSE,status=>VFD.Current_Status.eStatus);\nlclAlarm_Active.SET:=FALSE;\nIF Current_Alarm_EN THEN\n\tCASE Alarm.Status OF\n\t\teSensor_Status.HiHi_Alarm:\n\t\t\tVFD.Status.eStatus:=eVFD_Status.High_High_Current_Alarm;\n\t\t\tlclStatus:=VFD.Status.eStatus;\n\t\t\tlclAlarm_Active.SET:=TRUE;\n\t\teSensor_Status.High_Warning:\n\t\t\tVFD.Status.eStatus:=eVFD_Status.High_Current_Warning;\n\t\t\t//lclStatus:=VFD.Status.eStatus;\n\t\t\t//lclAlarm_Active.SET:=TRUE;\n\t\teSensor_Status.Low_Warning:\n\t\t\tVFD.Status.eStatus:=eVFD_Status.Low_Current_Warning;\n\t\t\t//lclStatus:=VFD.Status.eStatus;\n\t\t\t//lclAlarm_Active.SET:=TRUE;\n\t\teSensor_Status.Low_Low_Alarm:\n\t\t\tVFD.Status.eStatus:=eVFD_Status.Low_Low_Current_Alarm;\n\t\t\tlclStatus:=VFD.Status.eStatus;\n\t\t\tlclAlarm_Active.SET:=TRUE;\n\tEND_CASE\nEND_IF;\n\nIF VFD.Output_Pct>50 AND VFD.Data.Motor_Current<0.1 THEN\n\tVFD.Status.eStatus:=eVFD_Status.No_Motor_Load_Detected;\n\tlclStatus:=VFD.Status.eStatus;\n\tlclAlarm_Active.SET:=TRUE;\nEND_IF;\n\nIF lclAlarm_Active.Q1 THEN\n\tVFD.Status.eStatus:=lclStatus;\n\tVFD_CMD_PCT:=0;\n\tVFD_Run:=FALSE;\nEND_IF;\n\n\nT_1m(PT:=T#60S);\nIF T_1m.Q AND VFD.Data.Drive_STATUS.Run THEN\n\tVFD.RunTime_Minutes:=VFD.RunTime_Minutes+1;\nEND_IF;\n","LineInfoPersistence":"(string)9d01ef71-0294-48ec-b114-187911936bbe_Impl_LineIds"}}}},"UniqueIdGenerator":"(string)126"}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","8ac092e5":"{8ac092e5-3128-4e26-9e7e-11016c6684f2}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}