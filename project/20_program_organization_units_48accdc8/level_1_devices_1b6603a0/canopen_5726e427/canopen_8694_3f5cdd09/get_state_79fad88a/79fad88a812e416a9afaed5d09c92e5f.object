start.Q THEN\n\tSetCommunicationState:=CanOpen.SetCommunicationState(_3SCOS.DED.DEVICE_TRANSITION_STATE.RESET);\nEND_IF;\n\n\n","LineInfoPersistence":"(string)79fad88a-812e-416a-9afa-ed5d09c92e5f_Impl_LineIds"}}}},"UniqueIdGenerator":"(string)55"}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","8ac092e5":"{8ac092e5-3128-4e26-9e7e-11016c6684f2}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}