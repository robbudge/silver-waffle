TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION_BLOCK FB_DilutionCalculator\nVAR_INPUT\n    Target_Dilution : REAL;  // Target conductivity (mS/cm, for example)\n    Target_Rate : REAL; // Target flow rate (e.g., liters per second)\n    Line1 : REAL;  \n\tLine2 :\tREAL;\nEND_VAR\n\nVAR_OUTPUT\n    Line1_Rate : REAL; // Flow rate of the source solution\n    Line2_Rate  : REAL; // Flow rate of the dilution water\n    Line1_Percentage  : REAL; // Percentage of the source in the final mix\n\tLine2_Percentage  : REAL; // Percentage of the Water in the final mix\nEND_VAR\n\nVAR\n    BlendFactor:REAL;\nEND_VAR","LineInfoPersistence":"(string)84d54f9d-a5a8-4e5b-9350-1d01954f50ef_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)74","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}