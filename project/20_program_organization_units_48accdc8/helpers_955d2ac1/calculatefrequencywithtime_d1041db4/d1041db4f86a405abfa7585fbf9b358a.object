(IN := TRUE);\nEND_IF;\n","LineInfoPersistence":"(string)d1041db4-f86a-405a-bfa7-585fbf9b358a_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION_BLOCK CalculateFrequencyWithTime\nVAR_INPUT\n    CurrentCount : DWORD;  // Current counter value\nEND_VAR\nVAR_OUTPUT\n    Frequency : REAL;      // Calculated frequency\nEND_VAR\nVAR\n    PreviousCount : DWORD; // Previous counter value\n    PreviousTime : TIME;   // Previous time in milliseconds\n    //CurrentTime : TIME;    // Current time in milliseconds\n    PulseCount : DWORD;    // Number of pulses since the last update\n    TimeDifference : REAL; // Time difference in seconds\n    Timer : TON;           // Internal PLC timer for time tracking\nEND_VAR\n","LineInfoPersistence":"(string)d1041db4-f86a-405a-bfa7-585fbf9b358a_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)67","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}