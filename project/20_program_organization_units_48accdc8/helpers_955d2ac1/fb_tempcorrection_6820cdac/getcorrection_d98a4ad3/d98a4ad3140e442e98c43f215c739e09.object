{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)d98a4ad3-140e-442e-98c4-3f215c739e09","ParentGuid":"(Guid)6820cdac-3538-4945-982a-7a5e894ffb51","Name":"(string)GetCorrection","Properties":{"@Type":"2c41fa04:IDictionary","@Value":{}},"TypeGuid":"(Guid)f8a58466-d7f6-439f-bbb8-d4600e41d099","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"f8a58466","@Value":{"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)\nIF ArraySize < 2 THEN\n    GetCorrection := 0.0;\n    RETURN;\nEND_IF\n\n// Extrapolate below range\nIF InputTemp <= Temp[0] THEN\n    x0 := Temp[0];\n    x1 := Temp[1];\n    y0 := Correction[0];\n    y1 := Correction[1];\n    GetCorrection := y0 + ((InputTemp - x0) * (y1 - y0)) / (x1 - x0);\n    RETURN;\nEND_IF\n\n// Extrapolate above range\nIF InputTemp >= Temp[ArraySize - 1] THEN\n    x0 := Temp[ArraySize - 2];\n    x1 := Temp[ArraySize - 1];\n    y0 := Correction[ArraySize - 2];\n    y1 := Correction[ArraySize - 1];\n    GetCorrection := y0 + ((InputTemp - x0) * (y1 - y0)) / (x1 - x0);\n    RETURN;\nEND_IF\n\n// Interpolate within range\nFOR i := 0 TO ArraySize - 2 DO\n    IF (InputTemp >= Temp[i]) AND (InputTemp <= Temp[i + 1]) THEN\n        x0 := Temp[i];\n        x1 := Temp[i + 1];\n        y0 := Correction[i];\n        y1 := Correction[i + 1];\n        GetCorrection := y0 + ((InputTemp - x0) * (y1 - y0)) / (x1 - x0);\n        RETURN;\n    END_IF\nEND_FOR","LineInfoPersistence":"(string)d98a4ad3-140e-442e-98c4-3f215c739e09_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)METHOD GetCorrection : REAL\nVAR_INPUT\n    InputTemp : REAL;\nEND_VAR\nVAR\n    i : INT;\n    x0, x1, y0, y1 : REAL;\nEND_VAR","LineInfoPersistence":"(string)d98a4ad3-140e-442e-98c4-3f215c739e09_Decl_LineIds"}}}}}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","f8a58466":"{f8a58466-d7f6-439f-bbb8-d4600e41d099}","string":"System.String"}}},"FormatVersion":"1.0"}