{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)1e12f65b-13eb-4808-86d5-46e65a4a4474","ParentGuid":"(Guid)9764d18b-97de-4c85-ba32-989002d56418","Name":"(string)Task_Transfer","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)21af5390-2942-461a-bf89-951aaf6999f1","@Value":"(Guid)14d3d776-0786-4517-99ed-3342c141b77e"}]}}}}]},"TypeGuid":"(Guid)f8a58466-d7f6-439f-bbb8-d4600e41d099","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"f8a58466","@Value":{"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)Task_TON();\n\n\n\nIF Local_Task<> Config.LTun.Task.eTask THEN\n\tLocal_task:=Config.LTun.Task.eTask;\n\tConfig.LTun.Status:=eLT_Status.Transfer_Validate;\n\tTargets.Volume_Out:=0;\n\tTask_Ton.IN:=FALSE;\nEND_IF;\n\nIF targets.Disable THEN\n\tConfig.LTun.Status:=eLT_Status.Transfer_Complete;\nEND_IF;\n\n\nCASE Config.LTun.Status OF\n\t\n\teLT_Status.Transfer_Validate:\n\t\tIF (Grant_To_Kettle.Ready AND\n\t\t\tPID_Grant_In.Ready AND\n\t\t\tPID_Grant_Out.Ready AND\n\t\t\tGrant_Supply.Ready AND\n\t\t\tGrant_Bottom.Ready AND\n\t\t\tGrant_Level.Ready AND\n\t\t\tPump.Ready ) THEN\n\t\t\tConfig.LTun.Status:=eLT_Status.Vorlauf_Ready;\n\t\t\tPID_Grant_In.PLC_Target:=Targets.Grant_Level;\n\t\t\tPID_Grant_Out.PLC_Target:=Targets.Grant_Level-5;\n\t\t\tPID_Temp.PLC_Target:=Targets.Temperature;\n\t\t\tConfig.LTun.Status:=elt_Status.Transfer_Ready;\n\t\t\ttVolume:=Discharge_Flow.PDO.Totaliser_1.EU;\n\t\t\tTask_Ton.IN:=FALSE;\n\t\t\t\n\t\tEND_IF;\n\t\t\n\telt_Status.Transfer_Ready:\n\t\tIF (Grant_To_Kettle.Valve_Actual>5.0 AND  Discharge_Flow.PDO.VolumeFlow.EU>Targets.Min_Flow) THEN\n\t\t\tConfig.LTun.Status:=elt_Status.Transfer_Active;\n\t\t\ttVolume:=Discharge_Flow.PDO.Totaliser_1.EU;\n\t\tEND_IF;\n\telt_Status.Transfer_Active:\n\t\tTargets.Volume_Out:=Discharge_Flow.PDO.Totaliser_1.EU-tVolume;\n\t\tConfig.LTun.Task_Eta:= TO_UDINT((Targets.Max_Volume-Targets.Volume_Out)/(Discharge_Flow.PDO.VolumeFlow.EU/60));\n\t\t\n\t\t\n\t\tIF (Discharge_Flow.PDO.Totaliser_1.EU-tVolume)>Targets.max_Volume THEN\n\t\t\tConfig.LTun.Status:=eLT_Status.Transfer_Complete;\n\t\t\tConfig.LTun.Task_Eta:=0;\n\t\tEND_IF;\n\t\t\n\t\tIF Discharge_Flow.PDO.VolumeFlow.EU<Targets.Min_Flow   THEN\n\t\t\t\tConfig.LTun.Status:=eLT_Status.Low_Flow;\n\t\t\t\tConfig.LTun.Task_Eta:=0;\n\t\tEND_IF;\n\n\t\t\n\t\nEND_CASE;\n\n\n\n\nIF Discharge_Flow.PDO.VolumeFlow.EU>Targets.Max_Flow   THEN\n\t\t\tPID_Grant_Out.PLC_Target:= Grant_Level.EU+0.25;\nEND_IF;\nIF Discharge_Flow.PDO.VolumeFlow.EU<Targets.Max_Flow AND PID_Grant_Out.PLC_Target> (Targets.Grant_Level-5) THEN\n\tPID_Grant_Out.PLC_Target:= PID_Grant_Out.PLC_Target-0.01;\nEND_IF;","LineInfoPersistence":"(string)1e12f65b-13eb-4808-86d5-46e65a4a4474_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)METHOD Task_Transfer\nVAR_INPUT\n\tTargets:REFERENCE TO cLT_Transfer;\nEND_VAR\n","LineInfoPersistence":"(string)1e12f65b-13eb-4808-86d5-46e65a4a4474_Decl_LineIds"}}}}}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","f8a58466":"{f8a58466-d7f6-439f-bbb8-d4600e41d099}","string":"System.String"}}},"FormatVersion":"1.0"}